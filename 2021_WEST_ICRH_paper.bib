  author = {Lerche, E. and Goniche, M. and Jacquet, P. and Van Eester, D. and Bobkov, V. and Colas, L. and Giroud, C. and Monakhov, I. and Casson, F.J. and Rimini, F. and Angioni, C. and Baruzzo, M. and Blackman, T. and Brezinsek, S. and Brix, M. and Czarnecka, A. and Cromb{\'e}, K. and Challis, C. and Dumont, R. and Eriksson, J. and Fedorczak, N. and Graham, M. and Graves, J.P. and Gorini, G. and Hobirk, J. and Joffrin, E. and Johnson, T. and Kazakov, Y. and Kiptily, V. and Krivska, A. and Lennholm, M. and Lomas, P. and Maggi, C. and Mantica, P. and Mathews, G. and Mayoral, M.-L. and Meneses, L. and Mlynar, J. and {Monier-Garbet}, P. and Nave, M.F. and Noble, C. and Nocente, M. and Nunes, I. and Ongena, J. and Petravich, G. and Petrzilka, V. and P{\"u}tterich, T. and Reich, M. and Santala, M. and Solano, E.R. and Shaw, A. and Sips, G. and Stamp, M. and Tardocchi, M. and Tsalas, M. and Valisa, M. and {JET Contributors}},
  year = {2016},
  month = mar,
  volume = {56},
  pages = {036022},
  issn = {0029-5515, 1741-4326},
  doi = {10/f3rsxw},
  abstract = {Ion cyclotron resonance frequency (ICRF) heating has been an essential component in the development of high power H-mode scenarios in the Jet European Torus ITER-like wall (JET-ILW). The ICRF performance was improved by enhancing the antenna-plasma coupling with dedicated main chamber gas injection, including the preliminary minimization of RF-induced plasma-wall interactions, while the RF heating scenarios where optimized for core impurity screening in terms of the ion cyclotron resonance position and the minority hydrogen concentration. The impact of ICRF heating on core impurity content in a variety of 2.5 MA JET-ILW H-mode plasmas will be presented, and the steps that were taken for optimizing ICRF heating in these experiments will be reviewed.},
  journal = {Nuclear Fusion},
  language = {en},
  number = {3}
}

@article{litaudon2013,
