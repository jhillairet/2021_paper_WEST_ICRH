@article{durodie2017,
  ids = {durodie2017-1},
  title = {{{ITER}}-like Antenna for {{JET}} First Results of the Advanced Matching Control Algorithms},
  author = {Durodi{\'e}, F. and Dumortier, P. and Blackman, T. and Wooldridge, E. and Lerche, E. and Helou, W. and Goulding, R.H. and Kaufman, M. and K{\v r}ivsk{\'a}, A. and Van Eester, D. and Graham, M.},
  year = {2017},
  month = nov,
  volume = {123},
  pages = {253--258},
  issn = {09203796},
  doi = {10/gcvbhj},
  abstract = {The ITER-Like Antenna (ILA) for JET is a 2 toroidal by 2 poloidal array of Resonant Double Loops (RDL). It features in-vessel matching capacitors feeding RF current straps in Conjugate-T (CT) manner, a low impedance quarter-wave impedance transformer and a service stub allowing hydraulic actuator and water cooling services to reach the aforementioned capacitors. A Second Stage Matching (SSM) trombone and stub circuit allows to match the chosen CT working impedance to 30 . Toroidally adjacent RDLs are fed from a 3 dB hybrid splitter. It is worth mentioning that while the in-vessel matching approach was not kept for the ICRF antenna for ITER, the other aspects of the design for ITER such as the use of short straps, the validation of the TOPICA coupling code as well as operation at {$>$}40 kV and {$>$}5 MW/m2 were confirmed by the ILA.},
  journal = {Fusion Engineering and Design},
  keywords = {\#nosource,Control,ICRH,Matching},
  language = {en}
}

